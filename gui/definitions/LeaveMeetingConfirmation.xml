<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.12"/>
  <object class="GtkMessageDialog" id="leaveMeeting">
    <property name="title" translatable="yes">Confirm leave meeting</property>
    <property name="text" translatable="yes">Are you sure you want to leave from the meeting?</property>
    <property name="window-position">GTK_WIN_POS_CENTER</property>
    <property name="modal">true</property>
    <property name="border_width">7</property>
    <property name="message-type">GTK_MESSAGE_QUESTION</property>
    <property name="buttons">GTK_BUTTONS_YES_NO</property>
  </object>
</interface>

